@charset "UTF-8";
/* 颜色变量 */
/**
 * 自定义颜色
 */
.searchLayout {
  position: relative;
  padding: 150rpx 30rpx;
  height: 500rpx;
}
.searchLayout .historyList {
  margin-top: 30rpx;
}
.searchLayout .historyList .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  padding: 30rpx 0;
  color: #333;
  border-bottom: 1px solid #efefef;
}

